import Foundation

func fahrenheitToCelsius(fahrenheit: Double) -> Double {

    return (fahrenheit - 32) * 5/9

}

func celsiusToFahrenheit(celsius: Double) -> Double {

    return (celsius * 9/5) + 32

}

// Example usage

let fahrenheitTemperature: Double = 32.0

let celsiusTemperature: Double = 0.0



let convertedCelsius = fahrenheitToCelsius(fahrenheit: fahrenheitTemperature)

let convertedFahrenheit = celsiusToFahrenheit(celsius: celsiusTemperature)



print("\(fahrenheitTemperature)째F is equal to \(convertedCelsius)째C")

print("\(celsiusTemperature)째C is equal to \(convertedFahrenheit)째F")
